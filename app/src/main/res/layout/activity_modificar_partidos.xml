<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#EC0909"
    android:foregroundTint="#FD0000"
    tools:context=".Eliminar_Partidos">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/jc" />

    <AbsoluteLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/id_nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="166dp"
            android:layout_y="142dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="77dp"
            android:layout_y="136dp"
            android:backgroundTint="#EF0000"
            android:text="Jornada"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="149dp"
            android:layout_y="610dp"
            android:onClick="modificar"
            android:text="MODIFICAR" />

        <Button
            android:id="@+id/button4"
            android:layout_width="109dp"
            android:layout_height="wrap_content"
            android:layout_x="268dp"
            android:layout_y="177dp"
            android:onClick="buscar"
            android:text="Buscar" />

        <EditText
            android:id="@+id/id_ciudad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="133dp"
            android:layout_y="508dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <EditText
            android:id="@+id/id_puntos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="136dp"
            android:layout_y="415dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <EditText
            android:id="@+id/id_puntos1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="135dp"
            android:layout_y="459dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <EditText
            android:id="@+id/id_equipo1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="135dp"
            android:layout_y="377dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <EditText
            android:id="@+id/id_fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="119dp"
            android:layout_y="335dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:inputType="text" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="58dp"
            android:layout_x="15dp"
            android:layout_y="17dp"
            android:onClick="salir"
            android:text="salir"
            android:textSize="16sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="55dp"
            android:layout_y="333dp"
            android:text="Fecha"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="55dp"
            android:layout_y="375dp"
            android:text="Equipo1"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="55dp"
            android:layout_y="414dp"
            android:text="Equipo2"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="54dp"
            android:layout_y="455dp"
            android:text="Puntos1"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="51dp"
            android:layout_y="505dp"
            android:text="Puntos2"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />
    </AbsoluteLayout>
</FrameLayout>